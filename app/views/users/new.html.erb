<h1>Sign Up</h1><br>

<h4>Please Sign Up below or <%= link_to 'Login', login_path %></h4><br>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br><br>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Create User" %>
    
<% end %>